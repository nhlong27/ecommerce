{
  "extends": [
    "next/core-web-vitals",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "rules": {
    "react/display-name": "off",
    "import/no-anonymous-default-export": "off",
    "prefer-const": "off",
    "no-explicit-any": "off",
    "ban-ts-comment": "off",
    "no-non-null-assertion": "off",
  }
}
